

<%= simple_form_for @company do |f| %>

  <%= f.input :name %>
  <%= f.input :logo %>
  <%= f.input :mission %>

  <%= f.input :tech_stack %>

  <%= f.input :hiring_saturday %>

  <%= f.input :hiring_sunday %>

  <%= f.input :hiring_contact %>

  <%= f.input :founded %>

  <%= f.input :team_size %>
  <%= f.input :pitch %>

  <%= f.input :right, as: :boolean %>
        
  <%= f.button :submit %>

<% end %>
